{
    "key": "open-in-cloud9",
    "name": "Open in Cloud9",
    "description": "Opens a BitBucket repository in a Cloud9 workspace.",
    "vendor": {
        "name": "Cloud9 Inc.",
        "url": "https://c9.io"
    },
    "baseUrl": "https://bitbucket.c9.io",
    "modules": {
        "oauthConsumer": {
            "clientId": "ddYsLDrJrkwNW6MCC9"
        },
        "webItem": [
            {
                "url": "https://c9.io/auth/bitbucket?r=https%3A%2F%2Fc9.io%2Fopen%2F%3Fclone_url%3D{repo_ssh_url}",
                "name": {
                    "value": "Open in Cloud9"
                },
                "location": "org.bitbucket.repository.actions",
                "key": "open-in-cloud9-action",
                "icon": {
                    "url": "https://bitbucket.c9.io/c9.png"
                }
            }
        ]
